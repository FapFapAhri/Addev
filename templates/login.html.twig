{% extends 'base.html.twig' %} {% block h1 %}Connexion{% endblock %} {% block title %}Connexion{% endblock %} {% block body %}

<div aria-label="breadcrumb">
    <ol class="breadcrumb mt-0">
        <li class="breadcrumb-item"><a href="{{ path('home') }}">Accueil</a></li>
        <li class="breadcrumb-item active" aria-current="page"><a href="#">Connexion</a></li>
    </ol>
</div>

<div class="jumbotron">
    {% if session.get('account') is null %}
    <h2>Connectez vous sur cette page</h2>


    {% else %}
    <h2>Heureux de vous revoir</h2>
    {% endif %}
</div>



{# message d'erreur #}
<div class="com-12 m-0">
    {% if errors is defined %} {% for error in errors %}
    <div class="alert alert-danger" role="alert">
        {{ error }}
    </div>
    {% endfor %} {% endif %} {# message de succes #} {% if success is defined %}

    <div class="alert alert-success" role="alert">
        {{ success }}
    </div>
    <div class="row d-flex justify-content-center">
        <a href="{{ path('adverts') }}" class="btn btn-primary btn-lg btn-block col-6">Retour aux annonces</a>
    </div>

    {% endif %}
</div>

{# si succes n'est pas deffinit #} {% if success is not defined %}

